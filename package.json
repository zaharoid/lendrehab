    "@types/better-sqlite3": "^7.6.13",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
